<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="trackinlogic.trans.pss.com.trackinlogic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".features.registration.LoginActivity"
            android:theme="@style/MyMaterialTheme" />
        <activity
            android:name=".features.registration.WelcomeActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".features.registration.RegistrationActivity"
            android:theme="@style/MyMaterialTheme" />
        <activity android:name=".features.registration.TimeAndCircleActivity" android:theme="@style/MyMaterialTheme"/>
        <activity android:name=".features.registration.CarrierRegistrationActivity" android:theme="@style/MyMaterialTheme"/>
        <activity android:name=".features.registration.ManualCarrierDetailsActivity" android:theme="@style/MyMaterialTheme"/>
        <activity android:name=".features.registration.DotSearchActivity" android:theme="@style/MyMaterialTheme"></activity>
        <activity
            android:name=".NavigationMenu"
            android:label="@string/title_activity_navigation_menu"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activities.LogSheetManagement" android:theme="@style/MyMaterialTheme" />
        <activity android:name=".activities.LogSheetActivity" android:theme="@style/MyMaterialTheme"/>
        <activity android:name=".activities.LogBookActivity" android:theme="@style/MyMaterialTheme"/>
    </application>

</manifest>